{
  "name": "wave2_rn_poc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "cd android && ./gradlew clean",
    "simulator": "open -a simulator",
    "open-ios-project": "cd ios && open wave2_rn_poc.xcworkspace",
    "debug": "cd android && ./gradlew clean && ./gradlew assembleDebug",
    "release": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "pr":"cd android/app/build/outputs/apk/release && python3 -m http.server",
    "pod": "cd ios && pod install",
    "pod-deintegrate": "cd ios && pod deintegrate",
    "rm-pod-lock": "cd ios && rm -rf Podfile.lock"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.7",
